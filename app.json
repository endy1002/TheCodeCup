{
  "expo": {
    "name": "TheCodeCup",
    "slug": "thecodecup",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "@react-native-async-storage/async-storage"
    ],
    "android": {
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codecup.app"
    },
    "extra": {
      "storageConfig": {
        "enableFallback": true,
        "retryAttempts": 3
      }
    }
  }
}
