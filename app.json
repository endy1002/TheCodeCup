{
  "expo": {
    "name": "TheCodeCup",
    "slug": "thecodecup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_square.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B4513"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-font"
    ],
    "android": {
      "package": "com.codecup.thecodecup",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_square.png",
        "backgroundColor": "#8B4513"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codecup.app"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "storageConfig": {
        "enableFallback": true,
        "retryAttempts": 3
      },
      "eas": {
        "projectId": "54d85898-55e5-4231-be5e-7af04f5041be"
      }
    }
  }
}
